<h1>Here are the List of breeds we specialize in!</h1>
<h1>List of Dog Breeds</h1>

<%= form_with(url: dog_breeds_path, method: 'get') do %>
  <%= text_field_tag :query, params[:query] %>
  <%= submit_tag 'Search' %>
<% end %>

<table>
  <tr>
    <th>Name</th>
  </tr>

  <% @dog_breeds.each do |dog_breed| %>
    <tr>
      <td><%= dog_breed.name %></td>
    </tr>
  <% end %>
</table>